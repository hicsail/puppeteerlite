#!/bin/bash

# Get current working directory
CWD=$(pwd)

# Start Constellation server
cd CONSTELLATION_HOME && node server.js &

# Run front-end
cd $CWD
python3 loginview.py

# Run back-end
curl -H "Content-Type: application/json" -d @request.json -X POST "http://128.31.25.45:8080/penguin-services-0.1.0/build" -o response.json

# Print gwl file
python3 tecan_json_to_gwl.py

echo 'Printed Tecan instructions.'
